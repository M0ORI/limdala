{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/css/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/js/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/login.html", "dest": "/login.html" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/contact.html", "dest": "/contact.html" },
    { "src": "/home", "dest": "/home.html" },
    { "src": "/home.html", "dest": "/home.html" },
    { "src": "/cards", "dest": "/cards.html" },
    { "src": "/cards.html", "dest": "/cards.html" },
    { "src": "/signup", "dest": "/signup.html" },
    { "src": "/signup.html", "dest": "/signup.html" }
  ]
}
